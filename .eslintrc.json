{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "xo",
  "overrides": [
    {
      "extends": ["xo-typescript"],
      "files": [".ts", ".tsx"]
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "@typescript-eslint/indent": "error",
    "quotes": "off",
    "@typescript-eslint/quotes": "warn"
  }
}
